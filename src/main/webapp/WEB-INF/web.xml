<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>musicServlet</servlet-name>
    <servlet-class>student.com.servlet.MusicServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>searchServlet</servlet-name>
    <servlet-class>student.com.servlet.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>singerServlet</servlet-name>
    <servlet-class>student.com.servlet.SingerServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>musicServlet</servlet-name>
    <url-pattern>/music</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>searchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>singerServlet</servlet-name>
    <url-pattern>/singer</url-pattern>
  </servlet-mapping>

</web-app>
